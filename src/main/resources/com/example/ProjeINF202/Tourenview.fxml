                        <Image url="@images/beforbuton.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </HBox>
      <Button fx:id="AddTourButon" layoutX="59.0" layoutY="146.0" mnemonicParsing="false" onAction="#onaddTourbuttonClicked" prefHeight="25.0" prefWidth="86.0" text="Neue Tour" textFill="#da7536">
