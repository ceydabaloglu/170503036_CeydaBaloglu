            <Font name="Arial Bold" size="29.0" />
         </font>
      </Label>
      <HBox layoutX="2.0" layoutY="235.0" prefHeight="97.0" prefWidth="194.0">
         <children>
            <Button fx:id="bevorbutton" mnemonicParsing="false" onAction="#onbevorbuttonClicked" prefHeight="21.0" prefWidth="44.0" textFill="#d0cbcb">
               <HBox.margin>
                  <Insets left="10.0" top="30.0" />
               </HBox.margin>
               <graphic>
                  <ImageView fitHeight="20.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true">
