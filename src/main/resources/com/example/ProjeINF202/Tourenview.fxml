<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #da7536 #da7536;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ProjeINF202.TourController">
   <children>
      <ListView fx:id="Tourlist" layoutX="200.0" layoutY="66.0" prefHeight="334.0" prefWidth="399.0">
         <Label fx:id="NameLabel" styleClass="Name" text=" " />
