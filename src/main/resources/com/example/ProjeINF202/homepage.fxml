<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ProjeINF202.homeController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="192.0" style="-fx-background-color: EAAC8b;">
         <opaqueInsets>
            <Insets left="30.0" right="30.0" />
         </opaqueInsets>
         <children>
            <Button fx:id="btnKunde" alignment="CENTER" mnemonicParsing="false" onAction="#onbtnKundeClick" text="Kunde">
               <VBox.margin>
                  <Insets top="40.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="btnBüropersonal" mnemonicParsing="false" text="Büro Personal">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="btnITManager" mnemonicParsing="false" text="IT Manager">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
            <ChoiceBox fx:id="btnInfo" prefWidth="150.0">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </ChoiceBox>
         </children>
      </VBox>
      <Pane layoutX="192.0" prefHeight="400.0" prefWidth="408.0" />
   </children>
</AnchorPane>
